{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for Claude Agent Platform",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-open",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "dialog:default",
    "notification:default"
  ]
}
